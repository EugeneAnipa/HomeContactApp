
<%- include('header'); -%>

<main>


<div class="justify-content">
<div><h3 class="text-center">Admin Sign Up</h3></div>
<form class="text-center form-floating mb-3
" method="post" action="/adminsignup">
<div class="form-group">
<lable>Email</lable><br>
<input type="email" name="email" required><br>
</div>
<div class="form-group">
<lable>First Name</lable><br>
<input type="text" name="firstname" required><br>
</div>
<div class="form-group">
<lable>Last Name</lable><br>
<input type="text" name="lastname" required><br>
</div>
<div class="form-group">
<lable>Role</lable><br>
<select class="mb-3" aria-label="Default select example">
  <option selected>Select Role here</option>
  <option value="1">Father</option></option>
  <option value="2">Mother</option>
  <option value="3">Guardian</option>
</select>
</div>
<div class="form-group">
<label>Password</label><br>
<input type="Password" name="password" required><br>
 
 </div>


 <div class="form-group">
<button type="submit" class="btn btn-primary">Sign Up</button>
 </div>
</form>
</div>
</main>


<%- include('footer'); -%>